{
    "name": "pu239/nxtgn",
    "description": "Pu-239 Next Generation Tracker",
    "require": {
        "php": "^8.2",
        "fideloper/proxy": "^4.4",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ]
    }
}
